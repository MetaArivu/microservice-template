#!/bin/bash
# -------------------------------------------------------------------------------------------
# @author: Araf Karsh Hamid
# -------------------------------------------------------------------------------------------
# Check Container Logs
# -------------------------------------------------------------------------------------------
ORG=`../../getServiceName 2`
SERVICE=`../../getServiceName`
CID=`cat $SERVICE.cid`
echo "Checking logs for Container = $SERVICE and Container ID = $CID"
docker logs $CID
